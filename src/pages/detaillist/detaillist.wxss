.title{
  font-size: 16px;
  line-height: 48px;
  color: #333;
  text-align: center;
}
.ul{
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  margin: 30rpx auto;
}
.ul .li{
  flex: 0 0 25%;
  text-align: center;
  padding-bottom: 20rpx;
}
.ul .li image{
  width: 130rpx;
  height: 130rpx;
  border-radius: 65rpx;
  margin: 0 auto;
}
.ul .li .name{
  color: #999;
}
.add_time{
  color: #E09142;
}