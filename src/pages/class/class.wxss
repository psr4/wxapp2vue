.head{
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 99;
}
.top{
  height: 96rpx;
  border-bottom: 10rpx solid #F0F0F0;
  z-index: 98;
}
.topbar {
  display: flex;
  height: 100rpx;
  align-items: center;
  width: 100%;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eee;
}
.topbar .icon-jiantou{
  font-size: 42rpx;
  margin: 0 32rpx;
  color: #3785FD;
}
.topbar .input{
  flex: 1;
  margin-right: 30rpx;
  background: #EFEFEF;
  height: 60rpx;
  border: 1rpx solid #eee;
  position: relative;
  display: flex;
}
.topbar .input input{
  width: 100%;
  height: 100%;
}
.topbar .input .icon-search{
  margin: auto 12rpx auto 27rpx;
  color: #808080;
} 


.category-wrapper {
  display: flex;
  align-items: center;
  height: 96rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eee;
  /* justify-content: space-around; */
  color: #8B8B8B;
  font-size: 28rpx;
  z-index: 100;
  background: #fff;
  text-align: center;
}
.category-wrapper view{
  width: 25%;
}
.category-wrapper .active{
  color: #FF9935;
}
.categoryTap{
  position: fixed;
  top: -104rpx;
  height: 0;
  width: 100%;
  opacity: 0; 
  transform:translate3d(0,-50%,0);    
  z-index: -100;
  box-sizing: border-box;
  transition:none;
}
.hide{
  display: none;
}

.translate{
  position: fixed;
  top: 104rpx;
  transition:height 0.1s cubic-bezier(0.62,0.18,0.01,0),top 0.1s cubic-bezier(0.22,0.48,1,0.8),opacity 0.1s ease-in-out,transform 0.1s ease-in-out;  
  height: 100%;
  opacity: 1;
  transform:translate3d(0,0,0);  
  width: 100%;
  z-index: -100;
} 
.item-top{
  background: #fff;
  padding: 18rpx 32rpx;
  font-size: 28rpx;
  min-height: 420rpx;
}
.item-bottom{
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
}
.cate-ul,ms-ul{
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  z-index: -99;
  justify-content: space-between;
}
.ms-ul{
  justify-content: flex-start;
}
.cate-title{
  color: #7c7c7c;
  line-height: 88rpx;
}
.cate-ul .li{
  flex: 0 0 20%;
  text-align: center;
  border:1rpx solid #D5D5D5;
  line-height: 65rpx;
  border-radius: 10rpx;
  margin-bottom: 25rpx;

}
.ms-ul .ms{
  margin-right: 32rpx;
}

.cate-ul .li.active{
  color: #FF9935;
}
.cate-ul .li.sq{
  flex: 0 0 45%;
}
.category-wrapper .active .icon-plus-select-down{

  display: inline-block;
}
.category-wrapper .active2{
  transform: rotate(-180deg);
  transition: all 0.2s linear;
}
