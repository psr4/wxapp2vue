.sortTap ul{
 
}
ul.c-show{
  display:flex;
  flex-wrap:wrap;
  padding: 0 10rpx;
  transition: all .2s ease-out;
}
ul.c-show li{
  flex: 0 0 auto;
  margin: 5px;
  padding: 5px 0;
}
ul.c-show li text{
  padding:0 30rpx;
  font-size:12px;
  color:#666;
  border-right:0.5px solid #efefef;

}
.category-wrapper .category scroll-view .swiper ul li {
  flex: 1 0 auto;
  box-sizing: border-box;
  font-size: 26rpx;
  
}

.category-wrapper .category scroll-view .swiper ul li text {
  background: white;
  padding: 19rpx 0 16rpx;
  display: inline-block;
}

.category-wrapper .category scroll-view .swiper ul li text::after {
  content: "";
  display: inline-block;
  width: 46rpx;
}

.category-wrapper .category scroll-view .swiper ul li.active {
  color: #3785fd;
}

.category-wrapper .more {
  flex: 0 0 auto;
  background: white;
  border-bottom:1rpx solid #efefef;

}

.category-wrapper .more image {
  width:38rpx;
  height:38rpx;
  padding:19rpx 20rpx 0;

}

.book-wrapper .book-desc{
  text-align: right;
}
.book-wrapper .book-desc .title {
  position: relative;
  display: inline-block;
  margin: 32rpx 45rpx;
  font-size: 32rpx;
  color: #333;
}

 .book-wrapper .book-desc .title:after {
  position: absolute;
  content: "";
  top: 50%;
  right: 0;
  width: 110%;
  height: 50%;
  box-sizing: border-box;
  border-radius: 50rpx;
  background: rgba(255, 153, 53, 0.4);
}
/* 商圈列表 */
.book-ul .book-li{
  overflow: hidden;
  display: flex;
  padding: 21rpx 30rpx;
  border-bottom: 1rpx solid #efefef;
}
.book-ul .book-li image{
  width: 168rpx;
  height: 163rpx;
  border-radius: 10rpx;
}
.book-ul .book-li .info{
  padding-left: 27rpx;
  flex: 1;
  overflow: hidden;
  font-size: 12px;
  color: #333;
}
.book-ul .book-li .info .tit{
  font-size: 32rpx;
  line-height: 42rpx;
  padding-top: 5rpx;
  color: #222;
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}
.book-ul .book-li .info  image.huore{
  width: 32rpx;
  height: 32rpx;
  margin: -5rpx 0 0 17rpx;
  vertical-align: middle;
}
.book-ul .book-li .info .desc{
  color:#999;
  overflow:hidden;
  text-overflow:ellipsis;
  line-height:24px;
  padding-bottom:20rpx;
  white-space:nowrap;

}
.book-ul .book-li .info .other{
  display: flex;
  color: #676767;
  line-height: 64rpx;
}
.book-ul .book-li .info .address{
  display: flex;
  color: #676767;
  line-height: 57rpx;
  font-size: 22rpx;
}
.book-ul .book-li .info .address image{
  width: 20rpx;
  height: 24rpx;
  margin: auto 10rpx auto 0;
}
.book-ul .book-li .info .other .cate{

  margin-right: 15rpx;

}
.book-ul .book-li .info .other .line{
  width: 1rpx;
  height: 24rpx;
  background: #eee;
  margin: auto 0;
  margin-right: 15rpx;
}
.distant{
  background: #f1f1f1;
  border: 1rpx solid #eee;
  border-right: none;
  border-radius: 8rpx;
  text-align: center;
  padding: 0 7rpx;
  color: #666;
  height:38rpx;
  line-height:38rpx;
  margin: auto 0;
}
